+private long 
+dviprt_pcl2_encode(dviprt_print *pprint,long s,int f)
+{
+  uchar far *exam;
+  uchar far *cptr;
+  uchar far *end;
+  uchar far *src;
+  long total = 0;
+  
+  src = pprint->psource;
+  exam = src;
+  cptr = pprint->poutput = pprint->encode_buffer;
+  end = exam + s;
+  
+  for ( ; ; ) {
+    uchar test = *exam++;
+    int len;
+    while ((test != *exam) && (exam < end))
+      test = *exam++;
+    if (exam < end) exam--;
+    len = exam - src;
+    if (f) {
+      while (len > 0){
+        int i;
+        int count = len;
+        if (count>127) count=127;
+        *cptr++=count-1;
+        for (i = 0 ; i < count ; i++) *cptr++ = *src++;
+        len -= count;
+        total += (count+1);
+      }
+    }
+    else {
+      total += len + ((len/127)+(len%127 ? 1 : 0));
+    }
+    if (exam >= end) break;
+    exam++;
+    while ((test == *exam) && (exam < end))
+      exam++;
+    len = exam - src;
+    if (f) {
+      while (len > 0) {
+        int count = len;
+        if (count > 127) count = 127;
+        *cptr++ = (257-count);
+        *cptr++ = test;
+        len -= count;
+        total += 2;
+      }
+    }
+    else {
+      total += ((len/127 + (len%127) ? 1 : 0)) * 2;
+    }
+    if (exam >= end) break;
+    src = exam;
+  }
+  return total;
+}
