+int
+dviprt_unsetbuffer(dviprt_print *pprint)
+{
+  if (pprint->tempbuffer_f) {
+    BufferFree(pprint->encode_buffer);
+    pprint->tempbuffer_f = 0;
+  }
+  return 0;
+}
