+    uchar *pbuf = info.readbuf;
+    uchar *pchar;
+    
+    if (fgets(info.readbuf,info.readbuf_size,ifp) == NULL) break;
+    info.line_no++;
+    {
+      int len = strlen(pbuf);
+      if ((pbuf[0] < 'a' || pbuf[0] > 'z') && pbuf[0] != '_') {
+        while (pbuf[len-1] != '\n') {
