+		  (unsigned char)(color >> 8 & 0xff),
+		  (unsigned char)(color & 0xff));
+    lputs(s, obuf);
+    lputs(s, ESC_GS "3;2;1;0;0cpE" ESC_GS "1;2;1;0;0cpE" ESC_GS "5;2;1;0;0cpE");
