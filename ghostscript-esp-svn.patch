+private long 
+dviprt_hex_encode(dviprt_print *pprint,long s,int f)
+{
+  if (f) {
+    uchar far *w;
+    uchar far *buf;
+    static uchar hex[] = "0123456789ABCDEF";
+    int i=s;
+    w = pprint->poutput = pprint->encode_buffer;
+    buf = pprint->psource;
+    while (i-->0) {
+      *w++ = hex[(*buf>>4)&0x0f];
+      *w++ = hex[*buf&0x0f];
+      buf++;
+    }
+  }
+  return s*2;
+}
