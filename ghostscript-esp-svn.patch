+private int
+esmv_close(gx_device *dev)
+{
+    gx_device_vector	*const vdev = (gx_device_vector *) dev;
+    FILE		*f = vdev->file;
+
+    /* 終了処理コードは決め打ち */
+    (void)fprintf(f, ESC_GS "rhE" "\033\001@EJL \012@EJL EJ \012\033\001@EJL \012");
+
+    gdev_vector_close_file(vdev);
+
+    return 0;
+}
