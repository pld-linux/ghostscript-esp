diff -urN -x .svn espgs-8.15rc2/makesrcdist trunk/makesrcdist
--- espgs-8.15rc2/makesrcdist	1970-01-01 01:00:00.000000000 +0100
+++ trunk/makesrcdist	2005-03-14 00:04:17.847936928 +0100
@@ -0,0 +1,59 @@
+#!/bin/sh
+#
+# "$Id$"
+#
+# makesrcdist - make a source distribution of ESP Ghostscript.
+#
+
+echo "Getting distribution..."
+
+if test $# = 0; then
+	echo -n "Version number for distribution? "
+	read version
+else
+	version=$1
+fi
+
+cd /tmp
+
+if test $version = snapshot; then
+	url="https://svn.easysw.com/public/espgs/trunk"
+else
+	url="https://svn.easysw.com/public/espgs/tags/espgs-$version"
+
+	svn copy https://svn.easysw.com/public/espgs/trunk "$url" \
+		-m "Tag $version" || exit 1
+fi
+
+rm -rf espgs-$version
+svn export $url espgs-$version
+cd espgs-$version
+
+echo "Removing Subversion files and directories..."
+
+rm -f makesrcdist
+
+echo "Creating configuration script..."
+ln -s src/configure.ac .
+ln -s src/Makefile.in .
+autoconf -f
+
+cd ..
+
+echo "Making .tar.gz distribution..."
+
+gtar czf espgs-$version-source.tar.gz espgs-$version
+
+echo "Making .tar.bz2 distribution..."
+
+gtar cjf espgs-$version-source.tar.bz2 espgs-$version
+
+echo "Removing distribution directory..."
+
+rm -rf espgs-$version
+
+echo "Done!"
+
+#
+# End of "$Id$".
+#
