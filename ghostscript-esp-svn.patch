+private int
+dviprt_default_outputproc(uchar far *buf,long s,void *fp)
+{
+#ifdef __MSDOS_REAL__
+  while (s-- > 0) {
+    if (fputc(*buf++,fp) == EOF)
+      return CFG_ERROR_OUTPUT;
+  }
+  return 0;
+#else
+  return fwrite(buf,s,1,fp) == 1 ? 0 : CFG_ERROR_OUTPUT;
+#endif
+}
